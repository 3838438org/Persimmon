<TenFoldBlock>:
	#in_1: in_1
	out_1: out_1
	block_label: '10-fold'
	block_color: .133, .40, .40, 1
	#inputs: inputs
	outputs: outputs
	FloatLayout:
		id: content
		EmptyContent:
			pos: content.pos
			#Widget:
				#id: inputs
				#InputPin:
					#id: in_1
					#size_hint: None, None
					#size: '10dp', '10dp'
					#x: content.x + 5
					#y: content.y + content.height / 2
			Widget:
				id: outputs
				OutputPin:
					id: out_1
					size_hint: None, None
					size: '10dp', '10dp'
					x: content.x + content.width - 10 - 5
					y: content.y + content.height / 2
