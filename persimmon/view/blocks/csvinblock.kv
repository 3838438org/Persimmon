<CSVInBLock>:
	out_1: out_1
	block_label: 'csv input'
	block_color: .667, .224, .224
	outputs: outputs
	Button:
		pos: root.x + 30, root.y + 10
		size_hint: 0.7, 0.5
		text: 'File Dialog'
		on_press: root.file_dialog.open()
	Widget:
		id: outputs
		OutputPin:
			id: out_1
			x: root.x + root.width - 10 - 5
			y: root.y + root.height / 2
			color: root.block_color
