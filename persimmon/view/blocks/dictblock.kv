<DictBlock>:
	block_label: 'Dictionary'
	block_color: 0.5, 0.5, 0.5
	outputs: outputs
	dict_out: dict_out
	Widget:
		id: outputs
		OutputPin:
			id: dict_out
			x: root.x + root.width - 15
			y: root.y + root.height / 2 - 5
	TextInput:
		pos: root.x + 5, root.y + 5
		size_hint: 0.8, 0.6
		text: root.string
