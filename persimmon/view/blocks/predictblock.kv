<PredictBlock>:
	title: 'Predict'
	block_color: root.b.FIT_AND_PREDICT.value
	inputs: inputs
	outputs: outputs
	est_in: est_in
	data_in: data_in
	plain_out: plain_out
	Widget:
		id: inputs
		InputPin:
			id: est_in
			x: root.x + 5
			y: root.y + root.height - 35
			_type: root.t.CLASSIFICATOR
		InputPin:
			id: data_in
			x: root.x + 5
			y: root.y + 5
			_type: root.t.DATAFRAME
	Widget:
		id: outputs
		OutputPin:
			id: plain_out
			x: root.x + root.width - 15
			y: root.y + root.height / 2 - 5
			_type: root.t.ANY
