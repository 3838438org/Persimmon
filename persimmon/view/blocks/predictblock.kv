<PredictBlock>:
	block_label: 'Predict'
	block_color: 0.5, 0.5, 0.5
	inputs: inputs
	outputs: outputs
	est_in: est_in
	data_in: data_in
	plain_out: plain_out
	Widget:
		id: inputs
		InputPin:
			id: est_in
			x: root.x + 5
			y: root.y + root.height - 35
			color: .667, .424, .224
		InputPin:
			id: data_in
			x: root.x + 5
			y: root.y + 5
			color: .667, .224, .224
	Widget:
		id: outputs
		OutputPin:
			id: plain_out
			x: root.x + root.width - 15
			y: root.y + root.height / 2 - 5
			color: .5, .5, .5
