<CrossValidationBlock>:
	block_label: 'Cross Validation'
	block_color: .176, .533, .176
	inputs: inputs
	outputs: outputs
	data_input: data_input
	estimator_input: estimator_input
	cross_val_input: cross_val_input
	output: output
	Widget:
		id: inputs
		InputPin:
			id: data_input
			x: root.x + 5
			y: root.y + root.height - 15
			color: .667, .224, .224
		InputPin:
			id: estimator_input
			x: root.x + 5
			y: root.y + root.height / 2 - 5
			color: .667, .424, .224
		InputPin:
			id: cross_val_input
			x: root.x + 5
			y: root.y + 5
			color: .133, .40, .40
	Widget:
		id: outputs
		OutputPin:
			id: output
			x: root.x + root.width - 15
			y: root.y + root.height / 2

