<SVMBlock>:
	out_1: out_1
	block_label: 'SVM'
	block_color: .667, .424, .224
	outputs: outputs
	FloatLayout:
		id: content
		EmptyContent:
			pos: content.pos
			Widget:
				id: outputs
				OutputPin:
					id: out_1
					x: content.x + content.width - 10 - 5
					y: content.y + content.height / 2
					color: root.block_color
